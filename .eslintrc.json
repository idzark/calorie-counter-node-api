{
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": ["error", "never"],
        "func-names": 0,
        "linebreak-style": ["error", "windows"],
        "prefer-destructuring": [
            "error",
            {
                "object": false,
                "array": false
            }
        ],
        "no-underscore-dangle": 0
    }
}